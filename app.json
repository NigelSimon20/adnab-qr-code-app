{
  "expo": {
    "name": "Adnab Events App",
    "slug": "adnab-events-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adnab-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/adnab-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.user-qr-code-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adnab-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nijosimon.qrcode"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "026d5207-2ec9-4bd8-a961-921dd5f7597a"
      }
    }
  }
}
